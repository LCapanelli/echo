    "open:src": "tsc && node buildScripts/srcServer.ts",
    "ngrok": "ngrok http 3000",
    "share": "npx npm-run-all --parallel open:src ngrok",
    "lint": "esw --ext .ts ./src ./buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "build": "webpack --config ./webpack.config.js",
    "startx": "npm run build:dev",
    "build:dev": "webpack-dev-server --env dev --mode none",
    "build:prod": "webpack --env prod --mode production",
    "test": "mocha --reporter progress --require ./node_modules/babel-register ./buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
